#set($jsUrls= ["${rc.contextPath}/static/account/list.js","${rc.contextPath}/static/account/edit.js"])
<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try {
                ace.settings.check('breadcrumbs', 'fixed');
                var status="$!status";
            } catch (e) {
            }
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${rc.contextPath}/">首页</a>
            </li>
            <li class="active">$page_title</li>
        </ul>
        <!-- .breadcrumb -->
    </div>

    <div class="page-content">

        <div class="row">
            <div class="col-xs-12">
                <h3 class="header smaller lighter blue">$page_title</h3>

                <div class="table-responsive">
                    <table id="accountlist" class="table table-striped table-bordered table-hover">
                        <thead>
                        <tr>
                            <th class="col-lg-1">真实姓名</th>
                            <th class="col-lg-1">身份证号码</th>
                            <th class="col-lg-1">手机号</th>
                            <th class="col-lg-1">邮箱</th>
                            <th class="col-lg-1">纳税类型</th>
                            <th class="col-lg-1">税号</th>
                            <th>税率</th>
                            <th>状态</th>
                            <th class="col-lg-2">
                                <i class="icon-time bigger-110 hidden-480"></i>
                                创建时间
                            </th>
                            <th class="col-lg-2">
                                <i class="icon-time bigger-110 hidden-480"></i>
                                更新时间
                            </th>
                            <th class="col-lg-1">操作</th>
                        </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>

    </div>

</div>